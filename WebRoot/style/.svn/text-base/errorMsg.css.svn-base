.warning_box {
	background: url("images/warning.png") no-repeat scroll 15px 10px;
	border: 1px solid #EAE9EE;
	clear: both;
	margin: 0 auto;
	padding: 20px 20px 15px 60px;
	margin-left: 5px;
	margin-right: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
}

.valid_box {
	background: url("images/valid.png") no-repeat scroll 15px 10px;
	border: 1px solid #EAE9EE;
	clear: both;
	margin: 5px auto;
	padding: 20px 20px 15px 60px;
	margin-left: 5px;
	margin-right: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
}

.errorMessage {
	background: url("images/error.png") no-repeat scroll 15px 10px;
	border: 1px solid #EAE9EE;
	clear: both;
	margin: 0 auto;
	padding: 20px 20px 15px 60px;
	/* width: 540px; */
	margin-left: 5px;
	margin-right: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
}

.page {
	height: 30px;
	line-height: 30px;
	margin: 10px 0;
	padding: 5px;
	text-align: center;
	width: 100%;
}

.page a {
	border: 1px solid #E5E5E5;
	margin: 5px;
	padding: 5px;
	text-decoration: none;
}